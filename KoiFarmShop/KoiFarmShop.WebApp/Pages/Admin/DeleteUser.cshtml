@page
@model KoiFarmShop.WebApp.Pages.Admin.DeleteUserModel
<h2>Delete User</h2>

<p>Are you sure you want to delete this user?</p>

<div>
    <strong>@Model.UserToDelete.UserName</strong> - @Model.UserToDelete.Email
</div>

<form method="post">
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-page="/Admin/ViewAllUser" class="btn btn-secondary">Cancel</a>
</form>
